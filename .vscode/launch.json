    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickJavaProcess}"
        },
        
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch DmxLive",
            "request": "launch",
            "mainClass": "com.kevinisabelle.dmxLive.DmxLive",
            "projectName": "DmxLive",
            "args": "editor"
        }
    ]
}